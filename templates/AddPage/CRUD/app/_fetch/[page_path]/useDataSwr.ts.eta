// useSideMenuSwr.ts

import useSWR from 'swr'
import { fetcher } from '../fetcher'

export const useDataSwr = () => {
    // use swr
    const { data, error } = useSWR(`/api/<%= it.page_path %>/`, fetcher)

    return {
        data,
        isLoading: !error && !data,
        isError: error
    }

    //
}

export default useDataSwr

