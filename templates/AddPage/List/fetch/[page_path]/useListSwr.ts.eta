// useListSwr.ts

import useSWR from 'swr'
import { fetcher } from '@/fetch/fetcher'

export const useListSwr = () => {
    // use swr
    const { data, error } = useSWR(`/api/<%= it.page_path %>`, fetcher)

    return {
        data,
        isLoading: !error && !data,
        isError: error
    }

    //
}

export default useListSwr

