// useMenuSwr.ts

import useSWR from 'swr'
import { fetcher } from './fetcher'

export const useMenuSwr = () => {
    // use swr
    const { data, error } = useSWR(`/api/menu`, fetcher)

    return {
        data,
        isLoading: !error && !data,
        isError: error
    }

    //
}

export default useMenuSwr

